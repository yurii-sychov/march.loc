<div class="cases-searches">
    <div class="cases-searches__orderid">
        <input 
            id="casesSearchByOrderId"
            name="search_by_order_id"
            class="form-control cases-searches__orderid-input" 
            type="text"
            placeholder="Order ID"
            autocomplete="off"
        >
    </div>
    {% include '../../components/form/autocomplete-input.twig' with {
        id: "casesFilterNames",
        class: "cases-searches__names",
        name: "search_by_names",
        url: "/get-names",
        placeholder: "Plaintiff/Defendant Name",
